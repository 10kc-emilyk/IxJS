{
  "extends": "../tsconfig.json",
  "include": [
    "../src/**/*.ts",
    "../spec/**/*.ts"
  ],
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "allowJs": true,
    "declaration": false,
    "importHelpers": false,
    "noEmitHelpers": false,
    "noEmitOnError": false,
    "sourceMap": false,
    "inlineSources": false,
    "inlineSourceMap": false,
    "downlevelIteration": false,
    "baseUrl": "../",
    "paths": {
      "ix/*": [
        "src/*"
      ]
    }
  }
}
